    .triangle {
  width: 0;
  height: 0;
  border-left: 20px solid transparent;
  border-right: 20px solid transparent;
  border-bottom: 30px solid #08c4c7; /* Change the color as needed */
  position: absolute;
  top: 70%;
  left: 73%;
  transform: translate(-50%, -50%) rotate(0deg); /* Initial rotation angle */
  animation: rotateAnimation 1s linear infinite; /* Change the duration and timing function as needed */
}
.beam-container {
  width: 50%;
  height: 100px;
  overflow: hidden;
  position: relative;
}

.beam {
  width: 200%;
  height: 2px;
  background-color: #fff;
  position: absolute;
  top: 50%;
  animation: beamAnimation 4s infinite linear; /* Adjust duration as needed */
}
.br-8{
    background-color: #232323;
}
@keyframes beamAnimation {
  0% {
    left: -100%;
    opacity: 0;
  }
  25% {
    left: 0;
    opacity: 1;
  }
  75% {
    left: 100%;
    opacity: 1;
  }
  100% {
    left: 200%;
    opacity: 0;
  }
}

@keyframes rotateAnimation {
  from {
    transform: translate(-50%, -50%) rotate(0deg);
  }
  to {
    transform: translate(-50%, -50%) rotate(360deg);
  }
}

   </style>
